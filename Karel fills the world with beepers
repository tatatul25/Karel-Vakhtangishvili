 function main() {
 
  while(leftIsClear())
  {
   putBeeperLine();
   shemotrialdi();
   turnRight();
   move();
   turnRight();
  }
   putBeeperLine();
   
}
   
   
 function putBeeperLine(){
            putBeeper();
            while(frontIsClear()){
            move();
            putBeeper();
            }
   }
   function shemotrialdi(){
      turnAround();
      while(frontIsClear()){
         move();}
   }



